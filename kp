partial default alphanumeric_keys
xkb_symbols "kinesis" {

    include "kp(basic)"

    name[Group1]= "Katzenpfote on the Kinesis Advantage";

    // Alphanumeric section

    key.type = "ONE_LEVEL";

    // Left side of keyboard

    key <CAPS> { [ Shift_L ] };
    key <LFSH> { [ Caps_Lock ] };

    // Right side of keyboard

    key <BKSL> { [ Delete ]};
    key <AC11> { [ Shift_R ] };
    key <RTSH> { [ BackSpace ]};

    // Right thumb row
    key.type = "ONE_LEVEL";
    key <PGDN> { [ ISO_Level5_Shift ]};

    // Left thumb row
    key <BKSP> { [ ISO_Level3_Shift ] };
    key <DELE> { [ Control_L ] };
    key <END>  { [ Super_L ] };

    // Keyboard row number: -2
    key.type = "EIGHT_LEVEL";
    key <TLDE> { [ adiaeresis, Adiaeresis, U00E5, U00C5, U00E6, U00C6 ] };
    key.type = "TWO_LEVEL";
    key <LSGT> { [ udiaeresis, Udiaeresis ] };
    key.type = "THREE_LEVEL";
    key <AD11> { [ ssharp, U1D7D9, section ] };
    key.type = "EIGHT_LEVEL";
    key <AD12> { [ odiaeresis, Odiaeresis, U00F8, U00D8, U0153, U0152 ] };

};

partial default alphanumeric_keys
xkb_symbols "typematrix" {

    include "kp(basic)"

    name[Group1]= "Katzenpfote on the TypeMatrix";

    // Row: +2
    key.type = "TWO_LEVEL";

    key <TLDE> { [ Tab, ISO_Left_Tab ] };

    key.type = "ONE_LEVEL";

    key <AE12> { [ Alt_L ] };
    key <AE11> { [ Alt_R] };

    // Keyboard row number: +1
    key.type = "TWO_LEVEL";

    key <TAB>  { [ udiaeresis, Udiaeresis ] };

    // Keyboard row number: +1
    key.type = "EIGHT_LEVEL";

    key <BKSL> { [ odiaeresis, Odiaeresis, U00F8, U00D8, U0153, U0152 ] };

    // Keyboard row number: 0 (home row)
    key.type = "ONE_LEVEL";

    key <CAPS> { [ Shift_L ] };

    // Row: +1
    key.type = "TWO_LEVEL";

    key <TAB>  { [ udiaeresis, Udiaeresis ] };
    key <AD11> { [ odiaeresis, Odiaeresis, U00F8, U00D8, U0153, U0152 ] };
    key <AD12> { [ ISO_Level3_Shift ] };

    // Row: 0
    key.type = "THREE_LEVEL";

    key <AC11> { [ ssharp, U1D7D9, section ] };

    // Row: -1
    key.type = "EIGHT_LEVEL";

    key <BKSL> { [ adiaeresis, Adiaeresis, U00E5, U00C5, U00E6, U00C6 ] };

    // Row: -2
    key.type = "ONE_LEVEL";

    key <LALT> { [ ISO_Level3_Shift ] };
    key <RALT> { [ ISO_Level5_Shift ] };

    include "level3(modifier_mapping)"
    include "level5(modifier_mapping)"

};

partial default alphanumeric_keys
xkb_symbols "nogrid" {

    name[Group1]= "Katzenpfote on a staggered keyboard";

    // Alphanumeric section

    key <ESC>  { [ Escape ] };
    key <LCTL> { [ Caps_Lock ] };
    key <RALT> { [ Alt_L ] };

    key <SPCE> { [ space, space, Escape, Escape ] };

    // Keyboard row number: +2
    key <TLDE> { [ Home ] };
    key <AE01> { [ Super_L ] };
    key <AE02> { [ 7 ] };
    key <AE03> { [ 5 ] };
    key <AE04> { [ 3 ] };
    key <AE05> { [ 1 ] };
    key <AE06> { [ 9 ] };
    key <AE07> { [ 0 ] };
    key <AE08> { [ 2 ] };
    key <AE09> { [ 4 ] };
    key <AE10> { [ 6 ] };
    key <AE11> { [ 8 ] };
    key <AE12> { [ Tab, ISO_Left_Tab, Tab, ISO_Left_Tab ] };

};
